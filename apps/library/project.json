{
  "name": "library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/library/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/library",
        "main": "apps/library/src/main.ts",
        "tsConfig": "apps/library/tsconfig.app.json",
        "assets": [
          "apps/library/src/assets"
        ],
        "isolatedConfig": true
      },
      "configurations": {
        "development": {
          "webpackConfig": "apps/library/webpack.hmr.config.js"
        },
        "production": {
          "webpackConfig": "apps/library/webpack.config.js",
          "generatePackageJson": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "library:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "library:build:development"
        },
        "production": {
          "buildTarget": "library:build:production"
        }
      }
    }
  }
}
